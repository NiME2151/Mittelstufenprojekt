{
  "compilerOptions": {
    "target": "ES2016",
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
//    "noUnusedLocals": true,
//    "noUnusedParameters": true,
    "noImplicitAny": true,
    "baseUrl": "./src"
  }
}
